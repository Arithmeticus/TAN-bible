<TAN-A-div xmlns="tag:textalign.net,2015:ns" TAN-version="1"
    id="tag:kalvesmaki@gmail.com,2014:lxx.TAN-A-div">
   <head>
      <name>Div-based alignment of multiple versions of the Septuagint</name>
      <master-location>https://raw.githubusercontent.com/Arithmeticus/TAN-bible/master/TAN-A-div/lxx.TAN-A-div.xml</master-location>
      <rights-excluding-sources include="HEAD"/>
      <inclusion xml:id="HEAD">
         <IRI>tag:kalvesmaki@gmail.com,2014:bible.TAN-A-div</IRI>
         <name>Div-based alignment of multiple versions of the Bible</name>
         <location when-accessed="2015-10-02-04:00">HEAD.bible.TAN-A-div.xml</location>
      </inclusion>
      <source xml:id="lxx">
         <IRI>tag:kalvesmaki.com,2014:lxx-grc-1979-rahlfs.TAN-T.001</IRI>
         <name>Greek Septuagint, Rahlfs's edition</name>
         <location when-accessed="2014-10-23">../lxx.grc.rahlfs.xml</location>
      </source>
      <source xml:id="brenton">
         <IRI>tag:kalvesmaki.com,2014:lxx-eng-1851-brenton</IRI>
         <name>Brenton's English translation of the Septuagint</name>
         <location when-accessed="2014-10-23">../lxx.eng.brenton.1851.xml</location>
      </source>
      <declarations>
         <implicit-div-type-refs src="brenton lxx"/>
         <tokenization src="lxx" which="general-words-only-1"/>
         <tokenization src="brenton" which="general-1"/>
      </declarations>
      <agent include="HEAD"/>
      <role include="HEAD"/>
      <change when="2015-10-15" who="kalvesmaki">Started file</change>
   </head>
    <body>
       <realign>
          <anchor-div-ref src="lxx" ref="Mic 4 14 - Mic 5 14"/>
          <div-ref src="brenton" ref="Mic 5 1 - Mic 5 15"/>
       </realign>
    </body>
</TAN-A-div>
